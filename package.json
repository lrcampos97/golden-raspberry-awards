{
  "name": "golden-raspberry-awards",
  "version": "1.0.0",
  "description": "Gold raspberry awards API",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "prettier": "prettier -c ./src",
    "eslint": "eslint ./src",
    "prettier:fix": "prettier --write ./src",
    "eslint:fix": "eslint --fix ./src",
    "format": "pnpm run prettier:fix && pnpm run eslint:fix",
    "build": "rm -rf dist && node_modules/.bin/tsc --noEmit",
    "start": "pnpm prisma db push --force-reset > /dev/null && ts-node src/index.ts",
    "test": "NODE_ENV=test jest --verbose --runInBand -c jest.config.js",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "prisma:reset": "pnpm prisma db push --force-reset"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "prisma": "^5.16.2",
    "reflect-metadata": "^0.2.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2"
  }
}
